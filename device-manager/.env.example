# Environment Configuration Example
# Copy this file to .env and update with your actual values

# Domain Configuration
DEVICE_MANAGER_HOST=device-manager.myhost.pl
MONGO_HOST=mongo.myhost.pl

# Let's Encrypt Configuration
ACME_EMAIL=myemail@myhost.pl

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-this-secure-password-2025

# MQTT Configuration
MQTT_SERVER_PASSWORD=change-this-in-production-2025

# Use this when 1883 port is blocked to forward mqtt Connection
MQTT_HOST=http://mqtt.host.somthine.asdf

# Database Configuration
MONGO_ROOT_USERNAME=root
MONGO_ROOT_PASSWORD=change-this-secure-password-2025

# Database Connection
DATABASE_NAME=device_manager

# CORS
DEVICE_MANAGER_CORS_ORIGIN=http://web-client.myhost.pl