# .env.example
# Environment
NODE_ENV=development

# API Configuration
API_BASE_URL=http://localhost:3001

# MQTT Configuration
MQTT_BROKER_URL=ws://localhost:3001/mqtt
MQTT_RECONNECT_PERIOD=5000
MQTT_CONNECT_TIMEOUT=30000
MQTT_KEEPALIVE=60

# Storage
USE_SESSION_STORAGE=false

# Retry Configuration
RETRY_INTERVAL_MS=5000
MAX_RETRIES=5

# Feature Flags
ENABLE_LOGGING=true